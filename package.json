{
  "name": "project-start",
  "version": "0.0.1",
  "description": "Base files for new project!",
  "scripts": {
    "start": "npm-run-all --parallel webpack security-check browser-sync",
    "share": "npm-run-all --silent --parallel start public-share",
    "browser-sync": "browser-sync start --server src --files \"src/*\" --port 2121",
    "public-share": "lt --port 2121 --subdomain blancode",
    "webpack": "webpack -p",
    "security-check": "nsp check"
  },
  "author": "Anthony White",
  "license": "",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  },
  "devDependencies": {
    "babel-core": "6.17.0",
    "babel-loader": "^7.1.4",
    "babel-preset-react": "6.24.1",
    "browser-sync": "2.23.7",
    "eslint": "^3.19.0",
    "eslint-config-google": "0.9.1",
    "eslint-config-problems": "0.1.1",
    "eslint-plugin-fp": "2.3.0",
    "eslint-plugin-import": "2.0.1",
    "localtunnel": "1.8.1",
    "npm-run-all": "3.1.1",
    "nsp": "2.6.2",
    "webpack": "4.6.0",
    "webpack-cli": "2.0.15"
  }
